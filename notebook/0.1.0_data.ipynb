    "DATA_RAW_DIR = DATA_DIR.getsyspath('raw/')\n",
    "\n",
    "with zipfile.ZipFile(DATA_PATH,'r') as unzip_file:\n",
    "    unzip_file.extractall(DATA_RAW_DIR)"
   ]
